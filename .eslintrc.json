{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "log"] }],
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "@next/next/no-img-element": "off",
    "@next/next/no-page-custom-font": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "tests/load/*.js",
    "next-env.d.ts"
  ]
}
